{
    "executionRoleArn": "arn:aws:iam::AWS-ID:role/PJ-NAME-EcsTaskExecuteRole",
    "containerDefinitions": [
        {
            "name": "APP-NAME-task",
            "image": "<IMAGE1_NAME>",
            "memory": 512,
            "cpu": 256,
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ],
            "environment": [
                {
                    "name": "appname",
                    "value": "APP-NAME"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "APP-NAME-task"
}
